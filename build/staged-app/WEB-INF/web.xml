    <web-app  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
              version="3.1">
    <!--    <display-name>Archetype Created Web Application</display-name>-->
    <!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.login_jsp</servlet-name>
        <servlet-class>org.apache.jsp.login_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.home_jsp</servlet-name>
        <servlet-class>org.apache.jsp.home_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.error_jsp</servlet-name>
        <servlet-class>org.apache.jsp.error_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <servlet-class>org.apache.jsp.index_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.redirect_jsp</servlet-name>
        <servlet-class>org.apache.jsp.redirect_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.msg_002ddisplay_jsp</servlet-name>
        <servlet-class>org.apache.jsp.msg_002ddisplay_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.signup_jsp</servlet-name>
        <servlet-class>org.apache.jsp.signup_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.update_jsp</servlet-name>
        <servlet-class>org.apache.jsp.update_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.login_jsp</servlet-name>
        <url-pattern>/login.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.home_jsp</servlet-name>
        <url-pattern>/home.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.error_jsp</servlet-name>
        <url-pattern>/error.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <url-pattern>/index.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.redirect_jsp</servlet-name>
        <url-pattern>/redirect.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.msg_002ddisplay_jsp</servlet-name>
        <url-pattern>/msg-display.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.signup_jsp</servlet-name>
        <url-pattern>/signup.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.update_jsp</servlet-name>
        <url-pattern>/update.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>SignUpController</servlet-name>
        <servlet-class>Controller.SignUpController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpController</servlet-name>
        <url-pattern>/SignUpController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Controller.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RemoveAccountController</servlet-name>
        <servlet-class>Controller.RemoveAccountController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RemoveAccountController</servlet-name>
        <url-pattern>/RemoveAccountController</url-pattern>
    </servlet-mapping>

</web-app>

